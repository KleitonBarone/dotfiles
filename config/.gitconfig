[hub]
        protocol = https
[alias]
        co = checkout
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[core]
        excludesfile = ~/.gitignore
        editor = code --wait
        autocrlf = input
[apply]
        whitespace = nowarn
[mergetool]
        keepBackup = false
[difftool]
        prompt = false
[help]
        autocorrect = 1
[push]
        # See `git help config` (search for push.default)
        # for more information on different options of the below setting.
        #
        # Setting to git 2.0 default to suppress warning message
        default = simple
[init]
    defaultBranch = main
[fetch]
    prune = true
[rebase]
    autoStash = true
[commit]
    verbose = true
[pull]
    rebase = true
[rerere]
    enabled = true
[merge]
    conflictstyle = zdiff3
